<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<!-- 랫킨 반군 -->
	<!-- 첨단 기술 유출 작전부대 (= Advanced Technology Leaks Operations Unit) -->
	<TraderKindDef>
		<defName>Caravan_RatkinRebel_ATLOU</defName>
		<label>첨단 기술 유출 작전 소속 부대원들</label>
		<commonality>0.05</commonality>
    	<requestable>false</requestable>
		<stockGenerators>

            <li Class="StockGenerator_SingleDef">
              <thingDef>Silver</thingDef>
              <countRange>2750~7500</countRange>
            </li>

      		<li MayRequire="Ludeon.RimWorld.Royalty" Class="StockGenerator_SingleDef">
         	  <thingDef>PsychicAmplifier</thingDef>
       	 	  <countRange>1~3</countRange>
     		</li>

            <li MayRequire="Ludeon.RimWorld.Royalty" Class="StockGenerator_Category">
              <categoryDef>NeurotrainersPsycast</categoryDef>
              <thingDefCountRange>3~6</thingDefCountRange>
              <countRange>1~2</countRange>
            </li>

      		<li Class="StockGenerator_SingleDef">
         	  <thingDef>Schematic</thingDef>
       	 	  <countRange>4~8</countRange>
     		</li>

      		<li Class="StockGenerator_Techprints">
        	  <countChances>
          		<li>
           		  <count>8</count>
            	  <chance>2.5</chance>
          		</li>
        	  </countChances>
      		</li>

            <li Class="StockGenerator_Category">
              <tradeTag>WeaponRanged</tradeTag>
              <maxTechLevelGenerate>Spacer</maxTechLevelGenerate>
              <countRange>2~3</countRange>
			  <totalPriceRange>3500~12500</totalPriceRange>
			  <countRange>1~3</countRange>
            </li>

            <li Class="StockGenerator_Category">
              <categoryDef>WeaponsMelee</categoryDef>
              <maxTechLevelGenerate>Spacer</maxTechLevelGenerate>
              <thingDefCountRange>1~2</thingDefCountRange>
              <totalPriceRange>4000~10000</totalPriceRange>
              <countRange>2~4</countRange>
            </li>

      		<li MayRequire="Ludeon.RimWorld.Biotech" Class="StockGenerator_SingleDef">
        	  <thingDef>Mechlink</thingDef>
        	  <countRange>1~3</countRange>
      		</li>

     		 <!-- Genepacks -->
      		<li MayRequire="Ludeon.RimWorld.Biotech" Class="StockGenerator_SingleDef">
        	  <thingDef>Genepack</thingDef>
        	  <countRange>3~6</countRange>
      		</li>

      		<!-- Archite capsules -->
     		<li MayRequire="Ludeon.RimWorld.Biotech" Class="StockGenerator_SingleDef">
       		  <thingDef>ArchiteCapsule</thingDef>
        	  <countRange>2~4</countRange>
      		</li>

      		<!-- Buying -->
      		<li Class="StockGenerator_BuyExpensiveSimple" />

  			<li Class="StockGenerator_BuyTradeTag">
        	  <tag>ImplantEmpireCommon</tag>
      		</li>

      		<li Class="StockGenerator_BuyTradeTag">
        	  <tag>ImplantEmpireRoyal</tag>
      		</li>

      		<li Class="StockGenerator_BuyTradeTag">
        	  <tag>PsylinkNeuroformer</tag>
      		</li>

      		<li Class="StockGenerator_BuyTradeTag">
        	  <tag>ExoticBuilding</tag>
      		</li>

      		<li Class="StockGenerator_BuyTradeTag">
        	  <tag>MusicalInstrument</tag>
      		</li>

      		<li Class="StockGenerator_BuyTradeTag">
        	  <tag>UtilitySpecial</tag>
      		</li>

             <li Class="StockGenerator_BuyTradeTag">
              <tag>HiTechArmor</tag>
            </li>

            <li Class="StockGenerator_BuyTradeTag">
              <tag>Artifact</tag>
            </li>
	</TraderKindDef>
	
	<!-- 랫킨 비윤리적인 반군-->
